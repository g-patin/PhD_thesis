@inproceedings{berns_color_2011,
	title = {A color target for lighting and camera evaluation},
	abstract = {Lighting, in both the gallery and imaging studio, is a balance between preventive conservation and appearance. Color fidelity is a critical quality criterion, and current targets and metrics are inadequate for museum applications. A color target was formulated using the absorption and scattering properties of artist acrylic dispersion and conservation paints representative of historical and modern paintings. Twelve samples varying systematically in CIELAB hue at the maximum achievable chroma were used to evaluate color distortion and color clarity. Four neutrals were formulated with a range of color inconstancy under Illuminant A while matching under D65 and four metameric pairs were formulated to represent examples of deceptive restorative inpainting with poor pigment selection with respect to metamerism. This target, still computational, was tested for museum lighting and imaging applications, the results indicative of superior performance compared with conventional color targets and metrics.},
	booktitle = {{ICOM} {Committee} for {Conservation}, {ICOM}-{CC}, 16th {Triennial} {Conference}, {Lisbon}, 19-23 {September} 2011},
	publisher = {Critério},
	author = {Berns, Roy and Haddock, Marissa},
