Publisher: International Institute for Conservation of Historic and Artistic Works},
	keywords = {Conservation, Spectral reflectance, Colors, Light, Absorption spectra, Calibration, Environmental conservation, Excavation and methods, Laboratory methods, Lighting, Methodology and general studies, Optical reflection, Prehistory and protohistory, Sensors, Visible spectrum},
	pages = {85--98},
