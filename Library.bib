	pages = {2007--2056},
	file = {Full Text PDF:/home/gus/snap/zotero-snap/common/Zotero/storage/LIGSDI82/Ramarao et al. - 2003 - Moisture Diffusion Inside Paper Materials in the H.pdf:application/pdf},
}

