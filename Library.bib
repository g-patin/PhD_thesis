@article{kwan_demosaicing_2019,
	title = {Demosaicing of {Bayer} and {CFA} 2.0 {Patterns} for {Low} {Lighting} {Images}},
	volume = {8},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
