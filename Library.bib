	month = feb,
	year = {2018},
	note = {Number: 2
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {case studies, handheld, hyperspectral camera, sensor evaluation},
	pages = {441},
	file = {Full Text PDF:/home/gus/snap/zotero-snap/common/Zotero/storage/3G5EZXSF/Behmann et al. - 2018 - Specim IQ Evaluation of a New, Miniaturized Handh.pdf:application/pdf},
