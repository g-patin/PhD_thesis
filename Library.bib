	number = {7},
	urldate = {2020-04-29},
	journal = {Machine Vision and Applications},
	author = {Zeng, Y. and van der Lubbe, J. C. A. and Loog, M.},
	year = {2019},
	keywords = {colour change, drawing, van gogh, light damage, digital reconstruction},
	pages = {1229--1241},
}

