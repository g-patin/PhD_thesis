@article{down_yellowing_1986,
	title = {The yellowing of epoxy resin adhesives: report on high-intensity light aging},
	volume = {31},
	abstract = {Various room-temperature-cure epoxy resin adhesives suitable for use in glass conservation were tested for their tendency to yellow under high-intensity light. The results were extrapolated to representative museum conditions, taking into account both light- and thermally-induced contributions to yellowing. Life expectancy estimates, the length of time for a thin film of epoxy to yellow unacceptably, were obtained for each adhesive. Although the majority of products demonstrated an unacceptable level of stability, a few formulations were identified with less tendency to yellow than others. Recommendations for the use of specific epoxy resin adhesives are given. Un certain nombre d'adhésifs aux résines époxy thermodurcissables à froid employés dans la restauration des verres ont été expérimentés quant à leur tendance au jaunissement sous une lumière de grande intensité. Les résultats ont été extrapolés à l'atmosphère des musées, prenant en compte à la fois la lumière et la contribution des conditions de température au jaunissement. On obtient pour chaque adhésif des estimations de durée de vie, c'est à dire du temps que met un film mince d'époxy à jaunir au-delà d'une limite acceptable. Bien que la majorité des produits ne conviennent pas, on a pu cependant identifier quelques formules qui avaient moins tendance à jaunir que les autres. Quelques recommandations d'usage des résines époxy sont données.},
	number = {4},
	journal = {Studies in Conservation},
	author = {Down, Jane},
	month = nov,
	year = {1986},
	keywords = {part1.3, reciprocity principle, epoxy resin adhesives, glass conservation, high-intensity light aging},
	pages = {159--170},
}

