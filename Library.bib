}

@incollection{ankersmit_managing_2017,
	address = {Cham},
	series = {Cultural {Heritage} {Science}},
	title = {Managing {Indoor} {Climate} {Risks} in {Museums}},
	isbn = {978-3-319-34241-2},
	url = {https://doi.org/10.1007/978-3-319-34241-2_1},
	abstract = {In this chapter the history of climate control is presented. Discussing the climate in an unheated museum in the late nineteenth century, to the extremely dry environment in a heated building in the early twentieth century. Experience with collection preservation during the Second World War indicated the positive effect of a stable relative humidity. Since the mid twentieth century the set point for the relative humidity and temperature closely followed the technical development of climate systems. Resulting in an uncontrolled increase in energy demand. The effect of these adaptations on the building and climate control systems was enormous. Confidence in the technology grew, all though in practice numerous examples exist of malfunctioning of these systems. The overall risks to the moveable collections have increased to an almost unacceptable level.With this publication collection managers and stakeholders are assisted; by providing information that will allow responsible decisions about the museum’s indoor climate to be made. The focus is not only on the outcome, but also on the equally important process, which leads to that outcome. Nine consecutive steps are proposed to structure the decision making: valuation, assessment of the collection needs, the building needs, human comfort requirements, preparing for a balanced decision, understanding the building physics, climate control strategies and finally a cost-benefit analysis to make a choice.},
	language = {en},
	urldate = {2021-12-02},
	booktitle = {Managing {Indoor} {Climate} {Risks} in {Museums}},
	publisher = {Springer International Publishing},
	author = {Ankersmit, Bart and Stappers, Marc H. L.},
	editor = {Ankersmit, Bart and Stappers, Marc H.L.},
	year = {2017},
	doi = {10.1007/978-3-319-34241-2_1},
	keywords = {Decision making process, History of climate control},
	pages = {1--13},
}

@article{bulow_collection_2010,
	title = {Collection management using preservation risk assessment},
	volume = {33},
	issn = {1945-5224},
	url = {https://doi.org/10.1080/19455220903509960},
	doi = {10.1080/19455220903509960},
	abstract = {Between 2005 and 2007, The National Archives UK carried out an extensive preservation risk assessment of its collection of original government documents. Results have highlighted the effectiveness of policies and practices already in place. However, the assessment also provided evidence of current weaknesses, and helped prioritise necessary work to improve further the management of the collection. This paper details what actions have been taken at The National Archives to improve issues such as environmental conditions within storage areas, mitigating risk of physical damage to documents from handling, policies on the use of archival microfilm masters, and to reduce damage and loss of original records by substitution copying. Résumé «Gestion d’une collection en utilisant une évaluation des risques pour la conservation» Entre 2005 et 2007, les Archives Nationales de Grande Bretagne ont mené un vaste programme d’évaluation des risques liés à la conservation de sa collection de documents originaux du gouvernement. Les résultats ont mis en lumière l’efficacité des politiques et des pratiques déjà en place. Cependant, l’évaluation a aussi mis en évidence des faiblesses actuelles et a aidé à orienter les priorités de travail nécessaires pour améliorer davantage la gestion des collections. Cet article détaille les actions qui ont été menées aux Archives Nationales afin d’améliorer certains aspects comme les conditions environnementales à l’intérieur des zones de stockage, la minimisation des risques de dommages physiques sur les documents dus aux manipulations, les politiques sur l’utilisation des masters de microfilms pour l’archivage, et les actions menées pour réduire les dommages et les pertes d’enregistrements originaux en substituant des copies. Zusammenfassung “Sammlungsmanagement durch Risikoanalyse” Das britische Nationalarchiv führte zwischen 2005 und 2007 eine umfassende Analyse des Erhaltungsrisikos der Sammlung von originalen Regierungsdokumenten durch. Die Ergebnisse unterstrichen die Effektivität der Strategien und Maßnahmen, die in den vergangenen Jahren bereits durchgeführt wurden. Dennoch zeigte die Analyse auch Schwächen im derzeitigen Sammlungsmanagement, und half bei den nötigen Arbeiten, diese Situation zu verbessern und Prioritäten zu setzen. Dieser Artikel detailliert, welche Schritte am Nationalarchiv unternommen wurden, um die Bedingungen in den Depots zu verbessern; wie das Schadensrisiko bei der Nutzung verringert wurde; wie Regulierungen zum Gebrauch von Microfilmen formuliert wurden und wie Schäden und Verluste an Originalen durch Ersatzkopien reduziert wurden. Resumen “Manejo de colecciones utilizando la evaluación de prevención de riesgos” Entre los años 2005 al 2007, el Archivo Nacional del Reino Unido realizó una amplia evaluación sobre la preservación de riesgos en su colección de documentos gubernamentales originales. Los resultados han confirmado la efectividad de las prácticas y políticas que ya se estaban realizando. Sin embargo, la evaluación también hizo evidente las actuales debilidades y ayudó a priorizar los trabajos necesarios para mejorar el futuro manejo de la colección. Este artículo señala las acciones que se ha llevado a cabo el Archivo Nacional para mejorar temas tales como las condiciones ambientales dentro de las áreas de almacenamiento, la mitigación de riesgos por daños físicos en el manejo de los documentos, las políticas sobre el uso de los microfilmes maestro y cómo reducir daños y pérdidas de documentos originales sustituyéndolos por copias.},
	number = {1},
	urldate = {2021-12-02},
	journal = {Journal of the Institute of Conservation},
	author = {Bülow, Anna E.},
	month = mar,
	year = {2010},
	keywords = {preventive conservation, archives, risk assessment, collection management, preservation policy, preservation strategy},
	pages = {65--78},
}

