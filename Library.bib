@article{hallett_ultravioletfiltered_2003,
	title = {Ultraviolet‐filtered lighting and cellulose degradation: {Evaluating} the effect of light exposure on ethnographic collections},
	volume = {27},
	url = {https://doi.org/10.1080/01410096.2003.9995185},
	doi = {10.1080/01410096.2003.9995185},
	abstract = {Abstract This project explored the effect of ultraviolet‐filtered lighting on cellulosic materials. Ethnographic collections were used as a case study because they contain a diverse range of such materials, and are usually more impure as a result of less processing. Deterioration occurring by means of oxidation mechanisms, which are believed to predominate from exposure to visible light, was studied through scientific investigation. Colour measurement and chemical analysis were used to evaluate the extent of damage resulting from accelerated ageing designed to simulate exhibition at illumination levels slightly higher than those usually specified. The results indicated a difference in trend and scale of reaction between impure and pure model materials. Conclusions are given here regarding lighting levels for cellulosic collections in order to maximise access while managing the small amount of damage that must inevitably occur.},
	number = {1},
	journal = {The Conservator},
