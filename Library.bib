@article{knop_paper_2007,
	title = {Paper and board in closed boxes : alteration of water sorption capacity during cyclic temperature changes},
	volume = {28},
	abstract = {The role of temperature fluctuations on the water sorption capacity of the paper materials stored inside a closed solander box is explored. Two boxes of the same kind were filled with either a stack of corrugated board mounts or mat board mounts. The boxes were subjected to cyclic temperature changes between 28 and 56°C. The water sorption capacity of the boards overall gradually decreased over time, as did the amplitude of the absorption-desorption curve that occurred with each temperature change. The reduced water absorption capacity of the paper materials is assumed to be caused by an increase in density experienced by cellulose during wetting-drying cycles. While this was true for the different inserted materials, small differences were observed between them. 

L'étude porte sur l'influence de fluctuation de températures externes sur la capacité de sorption d'eau de matériel papier et carton placés dans des boîtes Solander. Pour faire cette étude on avait emboîté du papier, des enveloppes carton et du carton ondulé dans trois boîtes de structures similaires. Ces boîtes ont été placées dans une chambre climatisée pour y être soumises à des fluctuations cycliques de températures allant de 28 à 56° C, un cycle complet durant trois heures. La capacité de sorption d'eau du matériel a été indirectement influencée par la mesure de l'humidité dans l'espace d'air entre l'empilement des des boîtes. Les mesures effectuées ont montré qu'il y avait une baisse de la capacité de sorption d'eau selon la durée du traitement. Ceci ressort de la baisse de l'amplitude de la courbe d'humidité qui traduit la perte d'eau lors du réchauffement et le gain en eau lors du refroidissement du matériel placé en emboîtage. La réduction observée de capacité d'absorption d'eau peut être due à une compression des fibres de cellulose suite à une répétition de phases d'humidification et d'asséchement, on note à ce propos peu de différences notables dans les réactions des matériaux soumis à ce traitement. 

Der Einfluss von externen Temperaturschwankungen auf das Sorptionsvermögen von Papier und Kartonmaterialen gegenüber Wasser in geschlossenen Solanderboxen wird gemessen. Für das Experiment wurden Passepartouts aus Wellkarton oder Passepartoutkarton im Stapel in baugleiche Solanderboxen eingeschlossen. Die geschlossenen Boxen wurden in einer Klimakammer einem zyklischen Temperaturwechsel zwischen 28 und 56°C. Die Wasseraufnahmekapazität der Materialien wurde indirekt durch Messung der relativen Feuchte im Luftraum innerhalb der Stapel bestimmt. Die Messungen zeigen, dass das Wassersorptionsvermögen aller Materialien mit der Behandlungsdauer abnimmt. Das geht aus der Verkleinerung der Amplitude der Feuchtigkeitskurve hervor, die Wasserabgabe bei Erwärmung und Wasseraufnahme bei Abkühlung der eingeschlossenen Materialien widerspiegelt. Die beobachtete reduzierte Wasseraufnahmefähigkeit kann auf eine Verdichtung der Cellulosefasern als Folge der häufigen Wiederholung von Befeuchtung und Trocknung, zurückgeführt werden, wobei die verschieden Materialien sich nur geringfügig in ihrem Verhalten unterscheiden.},
	number = {3},
	journal = {Restaurator},
	author = {Knop, Anika and Banik, Gerhard and Schade, Udo and Brückle, Irene},
	year = {2007},
	keywords = {passepartout, moisture diffusion, mondriaan effect, mat board, water sorption},
	pages = {218--224},
}

@incollection{kohara_historic_2000,
