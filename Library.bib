	note = {Publisher: Routledge
\_eprint: https://doi.org/10.1080/01971360.2018.1543102},
	keywords = {HSI, Reflectance hyperspectral imaging, short-wave infrared imaging, spatial resolution, spectral resolution, SWIR imaging spectroscopy, test panel, underdrawings},
	pages = {16--36},
	file = {Full Text PDF:/home/gus/snap/zotero-snap/common/Zotero/storage/BM3TVHCL/Cucci et al. - 2019 - Short-wave infrared reflectance hyperspectral imag.pdf:application/pdf},
