	keywords = {MFT, lightfastness, microfade testing, anoxia, absorbance spectroscopy, Day-Glo, Daylight fluorescent pigments, fluorescence spectroscopy, optical brighteners},
	pages = {1--17},
	file = {Full Text PDF:/home/gus/snap/zotero-snap/common/Zotero/storage/AAW8ADM7/Sobeck and Smith - 2022 - Shedding Light on Daylight Fluorescent Artists’ Pi.pdf:application/pdf},
