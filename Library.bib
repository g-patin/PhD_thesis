@incollection{cumberland_modular_1992,
	address = {Pittsburgh},
	title = {Modular {Structure} for {Creating} an {Environmentally} {Controlled} {Space}},
	abstract = {A prefabricated, modular structure similar to a walk-in refrigerator can be used to create special high-quality museum collection storage space. These structures are becoming popular for storage of museum collections, either within an existing building or as a self-contained, stand-alone exterior building. Super-insulated structures can contribute to the preservation of museum collections by enclosing objects in a beneficial microenvironment. This environment is free of ambient light, dust, insects, and rodents and, most importantly, it provides stable relative humidity (RH) and temperature conditions. Stabilized, high-quality storage conditions reduce physical and chemical stress on objects and thereby slow down the rate of deterioration.},
