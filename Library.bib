@article{brokerhof_how_2015,
	title = {How can science connect with and contribute to conservation? {Recommendations} and reflections},
	volume = {60},
	abstract = {This paper reports the conclusions and recommendations of the working group that synthesized the discussions on ‘How can science connect with and be of greater benefit to conservation practice?’ during the ICCROM Forum on Conservation Science. The author reflects on these findings from her own perspective and experiences, and places them in the context of two major shifts in heritage research: the first, a shift in focus from conserving materials to managing meaning. The second, a shift in organizational structures from single, centrally funded heritage institutions towards diffuse networks which include new players who have no direct responsibility towards heritage. Both shifts are taking place in an environment of decreased funding and increased accountability to society. Science and conservation connect and contribute to each other most effectively if they together contribute to the societal benefits of heritage. In this regard, heritage science strategies can stimulate collaboration, and direct science and conservation towards innovative, applicable outcomes. Moreover, they can promote a transdisciplinary approach which connects social, economic and business sciences and stakeholders. They should also ensure the creation of sustainable nodes for consolidating knowledge within these dynamic networks.},
	number = {Sup2},
	journal = {Studies in Conservation},
	author = {Brokerhof, Agnes},
	year = {2015},
	keywords = {Conservation, Capacity networks, Heritage, ICCROM Forum, Research infrastructures, Science, Strategy},
	pages = {7--13},
}

@article{brokerhof_optimum_2008,
	title = {Optimum access at minimum risk: the dilemma of displaying {Japanese} woodblock prints},
	volume = {53},
	abstract = {This paper describes the evaluation of the lighting policy that the National Museum of Ethnology in Leiden, Netherlands, developed for the display of its nineteenth-century Japanese woodblock prints. Colour measurements during one year on exhibition showed changes that exceeded the limit of one ‘just noticeable change’ in 50 years specified in the policy. In a series of experiments, members of a test panel were all able to perceive the magnitude of colour difference measured in the prints during exhibition when presented with patches of colour side by side, but only half the panel could distinguish the same change when it was seen in the context of a multicoloured image. The change measured in the prints was judged by the panel to represent an average ‘loss of value’ of 9\%, and deemed to be acceptable over a period of 160 years. The outcome suggests exposure of the prints should be further limited for the benefit of future access.},
	number = {sup1},
	journal = {Studies in Conservation},
	author = {Brokerhof, Agnes W. and Reuss, Margrit and MacKinnon, Fiona and Ligterink, Frank and Neevel, Han and Fekrsanati, Farideh and Scott, Graeme},
	year = {2008},
	keywords = {light fading, lighting policy, decision process, japjapan woodblock prints, paper objects},
	pages = {82--87},
}

@article{brommelle_material_1956,
	title = {Material for a {History} of {Conservation}. {The} 1850 and 1853 {Reports} on the {National} {Gallery}},
	volume = {2},
	abstract = {Deux rapports des comités réunis en 1850 par le gouvernement Britannique en vue de l'aménagement de la National Gallery, ainsi que les commentaires verbaux des artistes, restaurateurs et autres spécialistes consultés, fournissent d'utiles informations sur les opinions et les techniques de conservation au milieu du 19e siècle. A ce moment l'état des peintures donnait des causes d'inquiétude et l'on critiquait le système de nettoyage. L'atmosphère enfumée de Londres était réputée mauvaise pour les peintures, et Sir Charles Eastlake proposait qu'on installe le système de conditionnement d'air adopté alors au Parlement. Ce système comprenait non seulement la purification par filtrage et lavage, mais était aussi le premier à utiliser l'humidification. Il fut proposé que les peintures soient recouvertes par des verres et leur dos enfermé sous des feuilles d'étain. À Londres, les restaurateurs enlevaient leurs vernis soit par frottement, soit à l'alcool généralement diluté de térébenthine. Peu de restaurateurs enlevaient complètement vernis ou surpeints. Ils évitaient de travailler sur des peintures où une restauration importante était requise (repeints et surpeints). Le vernis le plus utilisé était le mastic, et l'on connaissait les dangers des vernis à l'huile comme dc copal. Le célèbre 'Gallery varnish' (employé à la Gallery) et utilisé seulement par le restaurateur John Seguier, était un mastic mélangé d'huile de lin cuite avec des siccatifs au plomb. Il était surtout utilisé pour empêcher le 'chanci' mais il noircissait vite et était universellement critiqué. Le 'chanci' selon les témoignages, se produisait uniquement aux Galeries de Londres et de Dresden; il était dû aux gaz sulfureux de la fumée de charbon. Selon la théorie moderne, prouvée d'ailleurs par les rapports fournis aux comités, il résulterait du dépôt de sulfate d'ammonium provenant de l'atmosphère chargée de fumées de charbon et de vapeurs ammoniacales. D'après le peintre Dyce, le vernis Armenino, mélange d' 'olio d'abezzo' et d' 'olio di sasso' devait s'utiliser au-dessus du mastic habituel, et devait être retiré chaque année en entraînant les dépôts de suie et de graisses accumulés; il se dissolvait facilement à la térébenthine sans altérer la couche de mastic. Les vernis teintés étaient fréquemment utilisés en ce temps, mais les restaurateurs de la National Gallery n'en faisaient pas usage. Le vernis Dammar était très employé en Allemagne à ce moment, il était préféré au mastic pour les peintures anciennes. Il avait récemment été introduit à Florence. En Angleterre, un seul restaurateur l'employait régulièrement parce que le mastic vendu à cette époque était falsifié par des baumes. L'on ne pratiquait presque pas le rentoilage à la National Gallery par crainte des dommages, mais la technique en était bien connue. Le chimiste Michael Faraday avait fait des essais pour déterminer jusqu'à quel point les couches de peintures étaient protégées des gaz sulfureux par les vernis, il avait également fait des recherches sur les solvants. On discuta l'engagement d'un chimiste chargé de l'étude des vernis et des solvants.},
	number = {4},
	journal = {Studies in conservation},
	author = {Brommelle, Norman},
	year = {1956},
	keywords = {relative humidity, paintings, environment, pollution, cleaning, cadre, conservation préventive, environnement, frame, History of conservation, preventive},
	pages = {176--188},
}

