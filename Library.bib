@inproceedings{camuffo_environment_1991,
	address = {Bologna, Italy},
	title = {Environment and microclimate},
	abstract = {Environmental factors affecting the European Cultural Heritage both outdoor and indoor are considered, including on the mesoscale the geographic and climatic aspects; on the microscale, the microclimate and the interaction between works of art and the environment. Climatic and chemical factors have a combined weathering effect which becomes more dangerous when the time of wetness or atmospheric pollution goes up, or there is an increase in the concentration of catalysts. Rain transports acid substances from the atmosphere to monuments, especially in northern and central Europe. The meteorological nature of the precipitation, i.e. showers or drizzle is of great importance. The pattern of the white and black crusts on the monuments is caused by the way rainwater wets the surface. In southern Europe the photo-oxidants and the dry deposited gases and particles are dominant, especially in urban sites. The diurnal thermal cycles due to sunlight may mean that the monuments are subjected to great ranges of temperature and condensation - evaporation cycles. The microclimate, as well as being responsible for mechanical stress, thermohygrometric cycles and providing conditions suitable for biological degradation in low polluted areas, governs the rate of the dry deposition. Indoors, the microclimate depends on: external climatic factors, the way the environment is managed and presence of visitors. The problems related to artificial lighting, heating, air conditioning and cleaning are therefore discussed.},
	booktitle = {Science, technology, and {European} cultural heritage: proceedings of the {European} symposium, 13-16 {June} 1989},
	publisher = {Butterworth-Heinemann Publishers},
	author = {Camuffo, Dario},
	year = {1991},
	keywords = {environment, climatology, climatologie, acid rain, air conditioning, Air pollution, Climatologie, Climatology, microclimat nanoclimat, microclimate nanoclimate, pluie acide, Pollution atmospherique},
}

@book{camuffo_challenges_1987,
	title = {The challenges of the microclimate and the conservation of works of art"},
	abstract = {The microclimate and its dynamics play an important role in the deterioration processes of works of art, both directly (physical weathering) and indirectly (chemical and biological weathering). In particular, it induces environmental forcing cycles of both temperature and humidity and the thermohygrometric inertia of the work of art causes phase-lags with the forcing cycles which are responsible for exchanges of heat and moisture with the atmosphere. Therefore, mechanical stresses and deposition processes are originated. Rainfall and condensation supply liquid water which favours many others deterioration processes, especially in porous bodies. In this article the main results of some experimental campaigns are discussed, as well as some suggestions, given in order to obtain environmental conditions more convenient for a better conservation of the works of art.},
	author = {Camuffo, Dario; Bernardi, Adriana; Ongaro, A},
	year = {1987},
	keywords = {microenvironment, humidity, deterioration, alteration, microclimat, Climatologie, Climatology, Art object, deterioration diagnosis, deterioration humidity, microclimate nanoclimate ICCROM, Mural painting, nanoclimat, Objet art, Peinture murale},
}

@inproceedings{cane_developing_2011,
	title = {Developing a high-level risk assessment tool for heritage assets},
	abstract = {he Risk Assessment Tool (RAT) was developed in 2008 with the aim of enabling museums to create a snapshot of risk awareness through a self-assessment questionnaire. Following a pilot, it was decided that the scope of the project needed to alter to address risk awareness of the whole heritage asset, rather than just collections, with the aim of improving organisational sustainability and resilience. A multidisciplinary team used the recognised STEEPLED methodology to develop the Risk Awareness Preservation Tool (RAPT) on-line tool that was launched in 2010. The novel aspects of the tool are that it is easy to complete; provides an immediate assessment of awareness; provides help and direction in addressing areas of weakness; and the data gathered can be analysed to inform strategic planning at local, regional and national levels.},
