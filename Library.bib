@inproceedings{bogaard_explorations_2002,
	title = {Explorations of the role of humidity fluctuations in the deterioration of paper},
	abstract = {The control of relative humidity (RH) is generally recognized as an important consideration in the proper storage and display of paper materials. However, the amount that the humidity can fluctuate and the rate at which it can change without causing damage are still unknown. In this study, samples of cotton filter paper (Whatman No. 42) were held at room temperature in a chamber having RH that alternated between 25 and 75\% every two hours. Papers were removed from this chamber at intervals and analyzed for chemical deterioration, discoloration and strength loss. After roughly 2,200 humidity cycles, the paper had suffered a reduction in cellulose degree of polymerization (DP) to one-half its initial value, nearly its leveling-off DP, and its zero-span tensile strength had fallen to one-third the initial fiber strength. Chemical products were found to be consistent with a predominantly hydrolytic degradation. These results, and the course of the strength loss during degradation, suggest that the cycling humidity causes rapid stress-induced hydrolysis in this filter paper.},
	booktitle = {Works of art on paper: books, documents and photographs: techniques and conservation: contributions to the {Baltimore} {Congress}, 2-6 {September} 2002},
	publisher = {IIC},
	author = {Bogaard, John and Whitmore, Paul},
	year = {2002},
	keywords = {relative humidity, papier, paper, climate fluctuations, deterioration, hydrolyse, test},
	pages = {11--15},
}

@article{bommel_investigation_2005,
	title = {An investigation of organic red pigments used in paintings by {Vincent} van {Gogh} ({November} 1885 to {February} 1888)},
	volume = {3},
	abstract = {The investigation and identification of organic red pigments used in paintings by Vincent van Gogh was a part of the broader research programme into paintings by Van Gogh in his Antwerp and Paris period, November 1885 to February 1888, a project in collaboration between the Van Gogh Museum, the Netherlands Institute for Cultural Heritage and the Shell Research and Technology Centre in Amsterdam. Research results on these pigments used in this period were still lacking and, moreover, the researchers hoped for a better understanding of the ageing properties of the different organic red pigments. The ageing appears as deteriorated (faded) red lake areas, discoloration, fogging and changes in surface texture. It not only harms the art historian’s interpretation of the painting, but it also is a risk for its physical integrity. 41 Paintings of the Van Gogh Museum were examined visually and stereomicroscopically, 52.samples were taken for microscopic study of the paint cross-section and/or chemical analysis (High Performance Liquid Chromatography (HPLC) with PhotoDiode Array (PDA) and Fluorescence detection) and for Scanning Electron Microscopy with Energy Dispersive Spectroscopy (SEM-EDS). The techniques are described in an Appendix. All results are presented in a large table with a description of the object and the sample area, the visible deterioration, the microscopic observations, the SEM-EDS analysis, the HPLC analysis and the matrix in which the organic red pigment was identified. The history and way of preparation of the observed organic red pigments is described: cochineal (occurring both as a tin-based lake, together with fine starch grains and as an aluminium/calcium-based lake), madder and Kopp’s purpurin, and redwood. A pigment is either found as the only colouring matter or various pigments occur together in different compositions. Tables are given for (an indication of) the ratio between identified pigments and for the substrates of the colorants. In the course of the analyses a few other organic pigments were found, e.g. methyl violet, unidentified yellow compounds and indigo. Various inorganic pigments and extenders were found in combination with the organic red pigments, e.g. lead white, zinc white, barium sulphate, vermilion, ultramarine, emerald green and viridian. In several cases zinc and/or lead were found without an indication of a lead- or zinc-based pigment. The conclusion was that lead and/or zinc salts would be present as driers. The analytical results are discussed in relation to the condition of the sampled part of the painting. Especially the tin-based cochineal appears to have faded seriously, and almost completely when it was applied as a thin glaze. The use of low-grade pigments, that show serious degradation, shows that the painter had to economize on his materials.},
	journal = {Art Matters: Netherlands Technical Studies in Art},
	author = {Bommel, Maarten van and Geldof, Muriel and Hendriks, Ella},
	year = {2005},
	keywords = {colour change, Van Gogh, fading, brasilein, carminic acid, cochineal, degradation, HPLC, Kopp’s purpurin, madder, microscopy, organic red pigment, paint cross-section, paint sample, red lake, redwood, SEM-EDS, starch grains, substrate},
	pages = {111--137},
}

