@inproceedings{ansalone_vacuum_1996,
	title = {Vacuum conservation of archive and library materials},
	abstract = {A brief introduction considers the problems involved in, and research on. The conservation of large numbers of volumes and papers in libraries and archives. The paper presents a technology which can be used directly to prevent damage to contemporary library property, particularly newspapers. The method consists in encapsulating the material in multi-layered envelopes made of polyethylene, aluminium and polyester, in which a vacuum is created. Books and newspapers are thus protected from humidity, light, dust and any other agents of deterioration in the air. The second part of the work discusses the results of artificial ageing tests, which indicate that the method provides an encouragingly high degree of protection.},
	booktitle = {International conference on conservation and restoration of archive and library materials, {Erice} ({Italy}), {CCSEM}, 22-29 {April} 1996: preprints, vol. {I} and {II}},
	author = {Ansalone, Mario; Di Majo, Anna; Federici, Carlo},
