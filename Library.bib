	year = {1997},
	keywords = {light fading, paper, light damage, cellulose, photo deterioration, photo oxydation, yellowing},
	pages = {103--114},
}

@article{heenk_revealing_1994,
	title = {Revealing {Van} {Gogh}: {An} examination of his papers},
	volume = {18},
	doi = {10.1080/03094227.1994.9638586},
	abstract = {The examination of the physical qualities of the paper on which Van Gogh made his drawings, is a supplementary, but very important tool in identifying the relative date, function and authenticity of a drawing. It is the artist's consistent, organized way of working and use of his materials which makes the physical examination of his drawings so important. This article shows what information a sheet can convey and how this material evidence contributes to our understanding of the artist's intentions. Finally, this paper also describes how art historical investigations are sometimes hindered by the effects of conservation treatment L'examen du papier sur lequel Van Gogh faisait ses dessins, pour identifier ses caractéristiques physiques, est une aide supplémentaire mais très importante dans la détermination de la date relative, la fonction et l'authenticité d'un dessin. Que l'artiste travaillait et utilisait ses matériaux d'une façon conséquente et bien organisée rend l'examen technique de ses dessins si utile. Cet article démontre les renseignements que peut fournir un dessin et comment les caractéristiques physiques contribuent à notre connaissance des intentions de l'artiste. Enfin, l'article décrit en plus comment les recherches en histoire de l'art peuvent parfois être génées par les résultats d'un traîtement de restauration. Die Untersuchung der materiellen Merkmale von Van Goghs Zeichenpapieren ist ein ergänzendes, dafür aber außerordentlich wichtiges Instrument für die Datierung und für die Feststellung der Funktion und Authentizität der Zeichnungen. Es ist der Künstlers durchgängige, organisierte Arbeitsweise und Verwendung von Materialien, welche die materielle Untersuchung seiner Zeichnungen so relevant macht. Dieser Aufsatz verdeutlicht, welchen Informationsgehalt ein Blatt besitzt, und wie diese offenkundige Materialität zu dem Verständnis der künstlerischen Absichten beiträgt. Zuletzt beschriebt der Aufsatz auch, wie kunsthistorische Untersuchungen mitunter durch die Ergebnisse restauratorischer Behandlungen behindert werden},
	number = {1},
	journal = {The Paper Conservator},
	author = {Heenk, Liesbeth},
	year = {1994},
	keywords = {Van Gogh, drawing, dessin, examination, ppaper},
	pages = {30--39},
}

@article{heller_organic_1983,
	title = {Organic fatique-resistant photochromic imaging materials},
	volume = {130},
