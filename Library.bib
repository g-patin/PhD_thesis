@mastersthesis{tragni_use_2005,
	title = {The use of ultraviolet-induced visible fluorescence for examination of photographs},
	abstract = {Ultraviolet radiation has been widely used in art conservation as a non-destructive examination and photographic technique, providing useful information about museum objects. A number of separate studies have shown promising applications of ultraviolet-induced visible fluorescence to examine photographs. However, no comprehensive study specific to photographic materials currently exists. The project evaluates the application and limitations of ultraviolet-induced visible fluorescence examination for the identification, characterization, condition assessment, and treatment of photographs. A comprehensive examination of photographic materials, from daguerreotypes to color prints, was carried out using shortwave and longwave ultraviolet radiation. The study explores how photographic materials or a composite of different photographic materials interact with ultraviolet radiation, attempts to provide explanations of the observed phenomena, and discusses the use of such observations for photograph conservation. The report provides guidelines for using ultraviolet radiation to examine and record fluorescence, by providing information about the theory of ultraviolet-induced visible fluorescence, the ultraviolet radiation sources, the literature references, and the working procedure, including safety, examination, documentation, and standardization issues. For instance, digital cameras proved to be efficient for documenting fluorescence, showing clear advantages over photographic films. A fluorescent target suitable for fluorescence photography of museum objects is also proposed. The research shows the wide range of fluorescence that occurs in photographic materials, and that it can produce relevant information, such as the detection of ultraviolet absorbing layer in color prints, some tarnish on daguerreotypes, or some coatings applied on various types of photographs. The research also shows that the technique can have relevant application to treatment. For example, prior examination under ultraviolet radiation can reveal the presence of unexpected materials or areas of differential aging. The report is supplemented by a library of images that provides clear visual references. This library can serve as the foundation for further studies on the fluorescence occurring in photographs, for more in-depth examinations of certain fluorescence phenomena, and for further study of other examination techniques, such as infrared photography.},
	school = {George Eastman House \& Image Permanence Institute},
	author = {Tragni, Claire Buzit},
	year = {2005},
	keywords = {photography, paper, ultraviolet, UV, observation, coatings, fluorescence, albumen prints, collodion, daguerreotype tarnish, gelatin prints, optical brightening agents},
}

@article{trigueiro_when_2018,
	title = {When anthraquinone dyes meet pillared montmorillonite: {Stability} or fading upon exposure to light?},
	volume = {159},
	doi = {10.1016/j.dyepig.2018.06.046},
	abstract = {Hybrid pigments have attracted great interest due to their stability and physicochemical properties that can be used in paintings, artworks and in the field of cultural heritage. The study of new materials includes the search for new pigments by immobilization of the dyes in resistant substrates such clays minerals to get new colour fonts and materials with high stability. Organic-inorganic based lake pigments, at different pH, have been prepared. Ti- and Al-pillared montmorillonite were synthesized and loaded with carminic acid (CA) and alizarin (Aliz) organic dyes. The pillaring process was investigated by X-ray diffraction, textural analysis, transmission electronic microscopy and solid state nuclear magnetic resonance of 27 Al. The interactions between the organic guest and the inorganic host were highlighted by infrared, 13 C and 27 Al solid state magnetic nuclear resonance and time resolved fluorescence spectroscopies. The spectra support complex formation between the chromophores and the inorganic matrix. The colours of the pigments prepared were pH dependent. Lake pigments based on Al-Pillared Mt were more stable under light than Ti-based hybrid, even in oil painting formulations.},
	journal = {Dyes and Pigments},
	author = {Trigueiro, Pollyana and Pereira, Francisco A.R. and Guillermin, David and Rigaud, Baptiste and Balme, Sebastien and Janot, Jean-Marc and dos Santos, Ieda M.G. and Fonseca, Maria G. and Walter, Philippe and Jaber, Maguy},
	year = {2018},
	keywords = {light fading, PhD, colour change, color, Carminic acid, red, Alizarin, Fluorescence, Hybrid pigment, NMR, Photoaging, Pillared clay},
	pages = {384--394},
}

