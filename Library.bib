@phdthesis{del_hoyo-melendez_study_2010,
	type = {{PhD} {Thesis}},
	title = {A study on the action of light on objects of cultural interest by evaluating the light levels in the museum and performing micro-fading tests of materials},
	abstract = {Light is considered the most complex environmental parameter associated to the deterioration of museum collections. The high illumination levels sometimes required for appreciating a work of art may cause irreversible damage to its materials. The capacity of individual materials to absorb or reflect light needs to be determined since the extent of deterioration will depend on the chemical and physical nature of each object. Light-sensitive materials frequently found in museum collections generally include textiles, documents and works of art on paper. The complexity of the damaging effects of light to museum collections has caused an increase in the number of institutions interested in studying these phenomena over the past few years. This PhD thesis was focused in studying the effects of light on materials found in museum collections from macroscopic and microscopic points of view. The fading caused by intense light levels incident on the surface of an object can be detected by the naked eye. These degradation effects can also be identified and quantified at a microscopic level by using analytical methods. This study provided the opportunity of using a recently renovated museum as a space for field work in order to quantify and understand the behavior of light in its exhibition spaces. The building selected for the present study was the Old Patent Office Building in Washington, DC. The building, now called the Donald W. Reynolds Center (DWRC) for American Art and Portraiture, has undergone a six-year renovation and was reopened to the public in 2006. The space is shared between two museums: the Smithsonian American Art Museum and the National Portrait Gallery. This PhD thesis covers two major research areas. First, a light levels assessment of several exhibition spaces in the museum previously mentioned. Second, a technical study on the fading of materials found in a variety of laboratory samples and museum objects. The study has demonstrated the need for assessing the light levels in a museum and determining the amount of damage that light is capable of inducing in cultural heritage objects prior to the establishment of new exhibition guidelines},
	school = {Universidad Politécnica de Valencia, Facultad de Bellas Artes de San Carlos},
	author = {del Hoyo-Meléndez, Julio M.},
	year = {2010},
	keywords = {light fading, exhibition, museum lighting, light level, micro-fading test},
	file = {del Hoyo, 2010, MFT thesis.pdf:/home/gus/Documents/Bibliotheque/2010 pdf/del Hoyo, 2010, MFT thesis.pdf:application/pdf},
}

@article{del_hoyo-melendez_document_2015,
	title = {Document the light sensitivity of {Spanish} levantine rock art paintings},
	volume = {Volume II-5/W3},
	abstract = {A case study to evaluate the use of microfading spectrometry (MFS) for the study of colored systems found in prehistoric rock art paintings was conducted in the Cova Remígia rock-shelter, Castellón (Spain). This rock shelter is part of the rock art sites of the Mediterranean basin on the Iberian Peninsula included in UNESCO’s World Heritage List. Some of the paintings belonging to this group are exposed to environmental factors including natural daylight, wind and rain, depending on the time of the day and the season of the year. Therefore, their preservation is a major concern to stakeholders and researchers responsible for protecting and studying these prehistoric paintings. The experimental work in Cova Remigia focused on measuring the reflectance curves (400-700 nm) and determining the photostability of various areas containing red and black pigments on the rock art paintings. The preliminary results indicate that MFS is a suitable technique for studying the response to light of rock/pigment systems found in rock art sites. The advantages and limitations of the technique are discussed.},
	journal = {ISPRS Annals of the Photogrammetry, Remote Sensing and Spatial Information Sciences},
	author = {del Hoyo-Meléndez, J. M and Lerma, J. L. and López-Montalvo, E. and Villaverde, V.},
	year = {2015},
	keywords = {PhD, light, pigments, microfading spectrometry, exposure, photosensitivity, Spanish Levantine Rock art},
}

@article{del_hoyo-melendez_micro-fading_2012,
	title = {Micro-fading {Spectrometry}: {A} {Tool} for {Real}-time {Assessment} of the {Light}-fastness of {Dye}/{Textile} {Systems}},
	volume = {13},
	doi = {10.1007/s12221-012-1079-7},
	abstract = {Micro-fading spectrometry is a technique that combines visible reflectance spectroscopy and accelerated light aging testing. Therefore, it is a useful tool for determining the light-stability of dyes applied on textile substrates in a relatively short amount of time. Traditional accelerated light aging methods usually require controlling many variables such as time and intensity of illumination, correct positioning of the sample during irradiation, and reproducible spot selection for each subsequent spectrocolorimetric measurement. The use of micro-fade testing as an alternative way of conducting light-fastness assessments of dye/textile systems has been explored. The results indicate that the technique is suitable for testing the color stability of cultural heritage materials and industrially manufactured textiles since it is a direct and non-contact method that takes into account the aforementioned experimental variables.},
	number = {8},
	journal = {Fibers and Polymers},
	author = {del Hoyo-Meléndez, Julio M. and Mecklenburg, Marion F.},
	year = {2012},
	keywords = {light fading, textile, dyes, cultural heritage, colour change, MFT, micro-fading, color Degradation, light-fastness, Micro-fading spectrometry},
	pages = {1079--1085},
}

@article{del_hoyo-melendez_survey_2010,
	title = {A survey on the light-fastness properties of organic-based {Alaska} {Native} artifacts},
	volume = {11},
	abstract = {A series of light-fastness tests were conducted on a group of ethnographic objects that will be on exhibit at the Smithsonian Institution Arctic Studies Center, a recent addition to the Anchorage Museum at Rasmuson Center in Alaska. The objects surveyed belong to the collections of the Smithsonian National Museum of Natural History and the Smithsonian National Museum of the American Indian. This work was designed as a feasibility study on the use of a micro-fading tester as a non-contact and non-destructive technique to evaluate the light-stability of materials present in ethnographic collections. A broad range of objects containing a wide variety of materials were selected for the study. The materials investigated included a variety of dyes applied on silk, cotton, and wool substrates along with some unusual materials such as tanned skin and seal gut skin. The results from this investigation have allowed establishing exhibition recommendations taking into consideration the sensitivity of each object, light levels in the museum building, and estimated light exposures based on the duration of the exhibit. The micro-fading tester has proven to be a very useful tool for determining the light-stability of ethnographic materials without causing any harm to the objects. Objects containing equivalent materials are usually classified under a general category based on their probable sensitivity to light. However, micro-fading test results have permitted the detection of dissimilarities among some of these objects which could be associated to variations in prior fading histories, the quality of raw materials, and different preparation methods.},
	number = {4},
	journal = {Journal of Cultural Heritage},
	author = {del Hoyo-Meléndez, Julio M. and Mecklenburg, Marion F.},
	year = {2010},
	keywords = {light fading, Light-fastness, Alaska Native artifacts, Ethnographic collection, Micro-fading tester, objects},
	pages = {493--499},
}

