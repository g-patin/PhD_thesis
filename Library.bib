@article{druzik_illuminating_2004,
	title = {Illuminating {Alternatives} {Research} in {Museum} {Lighting}},
	volume = {19},
	url = {http://www.getty.edu/conservation/publications_resources/newsletters/19_1/news_in_cons1.html},
	abstract = {One area where preservation risks can probably be more effectively managed is museum lighting. In 2002 new research on museum lighting helped prompt an experts meeting, organized by the , that addressed questions involving the lighting of old master drawings. From that meeting’s discussions, it was evident that there were a number of strategies that could improve the display lifetime of works of art on paper. The GCI is now pursuing a research program on the subject.},
