@article{rea_relative_1991,
	title = {Relative visual performance. {A} basis for application},
	volume = {23},
	url = {https://doi.org/10.1177/096032719102300301},
	abstract = {Visual performance is defined in terms of the speed and accuracy of processing visual information. To evaluate illuminated tasks in terms of visual performance, it is necessary to have a valid computational model that relates measurable, salient aspects of the visual environment (e.g. target contrast) to measurable human responses (e.g. visual response time). A model of visual performance should be independent of the influence of non-visual factors as they influence speed and accuracy. It is also important to have practical application tools that can measure the salient aspects of the visual environment and compute (predict) visual performance for real tasks. This paper describes the basis for a visual performance model and how it can be applied using a computer imaging device. La performance visuelle est définie en termes de vitesse et de précision du traitement d'une information visuelle. Pour évaluer des tâches éclairées en termes de performance visuelle, il est nécessaire de disposer d'un modèle informatique valide qui fasse le lien entre des aspects mesurables importants de l'environnement visuel (par ex., le contraste de cible) et des réactions humaines mesurables (par ex., le temps de réaction visuelle). Un modèle de performance visuelle devrait être indépendant de l'influence de facteurs non visuels, lesquels influent sur la vitesse et la précision. Il est également important de pouvoir utiliser des outils d'application pratique qui peuvent mesurer les aspects mesurables importants de l'environnement visuel et calculer (prédire) la performance visuelle pour des tâches réelles. Ce document décrit les fondements d'un modèle de performance visuelle et la façon dont celui-ci peut être appliqué au moyen d'un système imageur. RES},
	journal = {Lighting Research and Technology},
	author = {Rea, Mark and Ouellette, Michael},
	month = sep,
	year = {1991},
}

