@phdthesis{marino_paints_2006,
	type = {{PhD} {Thesis}},
	title = {Paints quantified, {Image} analytical studies of preparatory grounds used by {Van} {Gogh}},
	abstract = {There is a general lack of knowledge concerning the materials and techniques applied by Van Gogh and his direct contemporaries. Substantial holdings of Van Gogh's work are kept in the Netherlands, providing a unique opportunity for in-depth study of a major late nineteenth-century artist and creating links to other painters of the period. Surprisingly little is known on the formulation of his commercially prepared paints in terms of pigments, binders, driers, stabilisers and extenders. Further, it is not known whether the painter added own materials to adjust the working properties of his tube paints. Advanced and established analytical techniques will be applied in collaboration with P4 and ICN to gain this information, using samples from selected paintings as well as surviving tube paints and a palette of the artist (Musée d'Orsay). Often the appearance of pictures by Van Gogh has been drastically altered by severe degradation of certain colour areas. Explanations will be sought for these phenomena, to help understand the current condition of his works. The relation between the composition and rheological properties of Van Gogh's paints will also be considered. How was the artist able to manipulate his materials to achieve particular painterly effects? Based on analytical findings and documentary research, historically accurate reconstructions of his paint systems will be made in collaboration with P5 to investigate this topic. New insights into the way that Van Gogh painted should bring us closer to understanding his pictures, since meaning and technique are intimately linked in his work. It is also possible that a highly specific characterisation of Van Gogh's painting materials and technique will provide useful criteria for dating and attribution. Institutions: Van Gogh Museum, Kröller-Müller Museum, AMOLF, ICN, Art Insitute of Chicago, National Gallery of Art (Washington), National Gallery (London), Musée d'Orsay, Laboratoire de recherche des musées de France, ETH Zürich.},
	school = {University of Amsterdam},
	author = {Marino, Beatrice},
	year = {2006},
	keywords = {pigments, paintings, imaging, Vincent van Gogh, grounds},
}

@inproceedings{marino_quantitative_2005,
