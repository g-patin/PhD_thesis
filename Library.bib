	keywords = {microclimate, microenvironment, relative humidity, papier, collection, conservation, analyse, environment, storage, climatology, paper, protection, analysis, test, book, climatologie, control, contrôle, dépôtt, essai, humidité relative, livre, measurement, mesure, microclimat, nnvironnement},
