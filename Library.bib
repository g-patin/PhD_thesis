@article{egerton_photochemistry_1970,
	title = {The photochemistry of dyes: (1) fundamental principles (2) some aspects of the fading process},
	volume = {86},
	shorttitle = {The photochemistry of dyes},
	url = {https://www.bcin.ca/bcin/detail.app?id=19555&lang=en&asq=&csq=paper+photochemical&csa=&ps=50&pId=2},
	abstract = {These two review papers summarize a lot of earlier papers in the light of present knowledge and for this reason could be useful to workers concerned with the fading of dyes. Fundamental photochemical processes are discussed in some detail with particular reference to quinone compounds. The significance of the lowest excited state in determining the sensitivity to light of a molecule is emphasized and the relation between color and sensitizing ability is discussed on this basis. Since the properties of the excited state can be affected easily by factors such as substituents and solvent properties it is suggested that the results obtained from using simple dye-solution systems to represent commercial dye-fabric systems must be examined critically. However, model systems are essential to provide more knowledge of the photochemistry of the compounds involved in this field. By employing flash photolysis, rigid solvents and electron spin resonance techniques, some indications have been obtained about the possible intermediates involved in the photochemical reactions of commercially dyed fibers. The effect of various factors (oxygen, temperature and humidity, atmospheric contaminants and concentration of dye) on the fading of dyes is discussed. The phototropic behavior of azo and thioindigoid dyes is described. The fading products resulting from the prolonged irradiation of dyes are discussed, as is the relation between the light-fastness of a dye and its chemical composition.},
	language = {English},
	number = {2},
	urldate = {2020-04-30},
	journal = {Journal of the Society of Dyers and Colourists},
	author = {Egerton, G. S. and Morgan, A. G.},
	year = {1970},
	keywords = {dyes, photochemistry, photochemical damage, photo deterioration, Fading and ageing processes},
	pages = {79--83 242--249},
}

@article{brokerhof_spread_2018,
