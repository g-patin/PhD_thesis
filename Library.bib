@article{thornton_toward_1997,
	title = {Toward a more accurate and extensible colorimetry. {Part} {IV}. {Visual} experiments with bright fields and both 10° and 1.3° field sizes},
	volume = {22},
	copyright = {Copyright © 1997 John Wiley \& Sons, Inc.},
	issn = {1520-6378},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.1002/%28SICI%291520-6378%28199706%2922%3A3%3C189%3A%3AAID-COL7%3E3.0.CO%3B2-N},
