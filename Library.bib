}

@inproceedings{ford_accelerated_2014-1,
	address = {Paris},
	title = {The accelerated light fading of iron gall inks in air, hypoxia and near-anoxia},
	isbn = {978-92-9012-410-8},
	abstract = {The response of over 100 historic iron gall inks on paper and parchment to continuous and interrupted accelerated light exposure in modified environments was investigated. In air, spontaneous post-fading colour reversion was consistently observed; however, in near-anoxic conditions it was suppressed and the initial response usually moderately accelerated. These and other results suggest a Prussian blue-like response involving photoreduction of Fe(III) to Fe(II) and reoxidation by molecular oxygen. The pre-reversion response was substantially independent of relative humidity. The results invalidate previous fading-rate estimates based on non-interrupted accelerated exposure studies, and the most straightforward interpretation suggests that low-oxygen display cases are unlikely to mitigate, and may even accelerate, the light fading of iron gall inks.},
	booktitle = {{ICOM}-{CC} 17th {Triennial} {Conference} {Preprints}, {Melbourne}, 15–19 {September} 2014},
