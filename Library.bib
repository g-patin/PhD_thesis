	issn = {1099-1476},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.1002/mma.6646},
	doi = {https://doi.org/10.1002/mma.6646},
	abstract = {Color measurement forms the basis of color management and color prediction theory and widely used in all aspects of color reproduction. Spectrophotometers are basic tools in color measurement; however, their photometric accuracy and precision, especially with a temporary resolution, have not been studied enough. In this work, we describe a specially developed experiment for the spectrophotometer's constancy assessment. We invite two common instruments to inspect X-Rite ColorMunki and X-Rite i1 Pro. Different precision tests basing on color difference and reflectance comparison are utilized. We obtain coincident results for both instruments. The measured parameters consistently change over time; however, the magnitude of these changes is small. The magnitude of the color difference obtained in the experiment for both devices does not exceed 0.45 (in relative dimensionless units dE).},
	language = {en},
	number = {17},
	urldate = {2021-01-26},
	journal = {Mathematical Methods in the Applied Sciences},
	author = {Tarasov, Dmitry A. and Milder, Oleg B.},
	year = {2020},
	keywords = {colour, spectrophotometer, color difference, estimation, analysis on metric spaces, rounding, spectral methods, theory of errors},
	pages = {9735--9744},
}

@article{amiri_use_2017,
	title = {Use of spectral sensitivity variability in reflectance recovery from colorimetric information},
	volume = {34},
	issn = {1084-7529, 1520-8532},
	url = {https://www.osapublishing.org/abstract.cfm?URI=josaa-34-7-1224},
	doi = {10.1364/JOSAA.34.001224},
	abstract = {There have been numerous methods for accessing the reflectance spectra; some methods enable one to obtain the spectra directly, such as using a spectrophotometer, hyper-spectral camera, and so forth. Even though the accuracy by which the spectra are obtained can be very high, the high price of these methods could pose an obstacle. On the other hand, there has been an interest in estimation of the spectral reflectance from colorimetric information that is more easily attainable; the accuracy in this case might not be as high but these methods are the most cost efficient. It is aimed, in this paper, to amalgamate the low price and high accuracy of the preceding approaches through relating spectral estimation to spectral sensitivity variability. The work is split into two major parts. The first part of the paper describes spectral sensitivity variability of humans and cameras in a theoretical manner and how it can be used in spectral recovery. In the second part, an attempt is made to extend the theory to real situations. At the end, it is shown how increasing the number of disparate colorimetric data of a specific object using the proposed methods aids in estimating spectral reflectance more accurately in a manner comparable to multi-spectral cameras.},
	language = {en},
	number = {7},
	urldate = {2021-01-26},
	journal = {Journal of the Optical Society of America A},
	author = {Amiri, Morteza Maali and Fairchild, Mark D.},
	month = jul,
	year = {2017},
	pages = {1224},
}

@article{amiri_strategy_2018,
	title = {A strategy toward spectral and colorimetric color reproduction using ordinary digital cameras},
	volume = {43},
	copyright = {© 2018 Wiley Periodicals, Inc.},
	issn = {1520-6378},
	url = {http://onlinelibrary.wiley.com/doi/abs/10.1002/col.22231},
	doi = {https://doi.org/10.1002/col.22231},
	abstract = {In this work, a methodology is introduced to use ordinary digital RGB cameras for the purpose of spectral and colorimetric color reproduction. First, it is attempted to recover the spectral reflectance from RGB camera response using different approaches, among which, it is shown that weighted nonlinear regression as performed better than other approaches. After analyzing the results, it is realized that although spectrally the results are satisfactory, there is still a significant colorimetric error left, that should be addressed. Therefore, in the second part of the article, different linear and nonlinear matrix transforms are used to change the RGB camera response to CIEXYZ tristimulus values under a specific condition. It is concluded that colorimetric error of the recovery can be reduced significantly when a separate path is used for colorimetric color reproduction.},
	language = {en},
	number = {5},
